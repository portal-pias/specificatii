															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="jDoc" type="jDocType" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation xml:lang="ro">Document justificativ (vezi Tip document justificativ)</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
