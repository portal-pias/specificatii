		<xs:attribute name="internmentType" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="ro">
					Tipul internarii. Acest atribut poate avea urmatoarele valori:
					1 -&gt; FARA BILET
					2 -&gt; BILET MEDIC DE FAMILIE
					3 -&gt; BILET MEDIC SPECIALIST AMBULATORIU
					4 -&gt; TRANSFER INTERSPITALICESC
					5 -&gt; LA CERERE
					9 -&gt; ALTE
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="1"/>
					<xs:enumeration value="2"/>
					<xs:enumeration value="3"/>
					<xs:enumeration value="4"/>
					<xs:enumeration value="5"/>
					<xs:enumeration value="9"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="internmentDiagCode" use="optional">
